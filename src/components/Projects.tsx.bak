            <div
              key={index}
              className="bg-gray-50 dark:bg-gray-800 rounded-xl p-5 shadow-sm hover:shadow-lg transition-all duration-300 transform hover:-translate-y-1 group cursor-pointer flex flex-col"
              onClick={() => {
                setSelectedProject(project);
                setModalOpen(true);
              }}
              style={{ animationDelay: `${index * 100}ms` }}
            >
              {/* Project Header */}
              <div className="flex items-start justify-between mb-3">
                <div className="flex items-center space-x-3 flex-1">
                  <div className="w-10 h-10 bg-blue-100 dark:bg-blue-900 rounded-lg flex items-center justify-center text-blue-600 dark:text-blue-400 group-hover:bg-blue-200 dark:group-hover:bg-blue-800 transition-colors flex-shrink-0">
                    {project.icon}
                  </div>
                  <div className="min-w-0">
                    <h3 className="text-base font-bold text-gray-900 dark:text-white group-hover:text-blue-600 dark:group-hover:text-blue-400 transition-colors truncate">
                      {project.title}
                    </h3>
                    <p className="text-xs text-blue-600 dark:text-blue-400 font-medium truncate">
                      {project.category}
                    </p>
                  </div>
                </div>
                {project.link && (
                  <a
                    href={project.link}
                    target="_blank"
                    rel="noopener noreferrer"
                    className="min-w-7 min-h-7 bg-white dark:bg-gray-700 rounded-full flex items-center justify-center text-gray-600 dark:text-gray-300 hover:text-blue-600 dark:hover:text-blue-400 hover:bg-blue-50 dark:hover:bg-blue-900/20 transition-colors shadow-sm flex-shrink-0"
                    aria-label={`Visit ${project.title} project`}
                    onClick={(e) => e.stopPropagation()}
                  >
                    <svg
                      className="w-3.5 h-3.5"
                      fill="none"
                      stroke="currentColor"
                      viewBox="0 0 24 24"
                      aria-hidden="true"
                    >
                      <path
                        strokeLinecap="round"
                        strokeLinejoin="round"
                        strokeWidth="2"
                        d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"
                      />
                    </svg>
                  </a>
                )}
              </div>

              {/* Short Overview */}
              <div className="mt-4 bg-gray-900 dark:bg-gray-900 rounded-lg p-5 border border-gray-700">
                <p className="text-sm font-semibold text-gray-200 uppercase tracking-wide mb-4">
                  30-Second Project Overview
                </p>
                
                {/* Metadata Grid */}
                {project.title === "AI-Driven Safety Compliance Platform" && (
                  <>
                    <div className="grid grid-cols-2 gap-4 mb-5 pb-5 border-b border-gray-700">
                      {/* Product Name */}
                      <div className="flex items-start space-x-3">
                        <svg className="w-5 h-5 text-blue-400 flex-shrink-0 mt-0.5" fill="currentColor" viewBox="0 0 20 20" aria-hidden="true">
                          <circle cx="10" cy="10" r="8" fill="none" stroke="currentColor" strokeWidth="2"/>
                          <circle cx="10" cy="10" r="3" fill="currentColor"/>
                        </svg>
                        <div>
                          <p className="text-xs text-gray-400 font-semibold uppercase tracking-wide mb-1">Product Name</p>
                          <p className="text-sm text-gray-100 font-medium">AI-Driven Safety Compliance Platform</p>
                        </div>
                      </div>

                      {/* Domain */}
                      <div className="flex items-start space-x-3">
                        <svg className="w-5 h-5 text-green-400 flex-shrink-0 mt-0.5" fill="currentColor" viewBox="0 0 20 20" aria-hidden="true">
                          <path d="M10.5 1.5H3a1.5 1.5 0 00-1.5 1.5v12a1.5 1.5 0 001.5 1.5h14a1.5 1.5 0 001.5-1.5V8.5m-5-5l5 5m0 0V3.5m0 5v5"/>
                        </svg>
                        <div>
                          <p className="text-xs text-gray-400 font-semibold uppercase tracking-wide mb-1">Domain</p>
                          <p className="text-sm text-gray-100 font-medium">Industrial Safety & Enterprise Compliance</p>
                        </div>
                      </div>

                      {/* Role */}
                      <div className="flex items-start space-x-3">
                        <svg className="w-5 h-5 text-purple-400 flex-shrink-0 mt-0.5" fill="currentColor" viewBox="0 0 20 20" aria-hidden="true">
                          <path d="M13 6a3 3 0 11-6 0 3 3 0 016 0zM18 8a2 2 0 11-4 0 2 2 0 014 0zM14 15a4 4 0 00-8 0v1h8v-1zM6 8a2 2 0 11-4 0 2 2 0 014 0zM16 18v-3a5.972 5.972 0 00-.75-2.906A3.005 3.005 0 0119 15v3h-3zM4.75 12.094A5.973 5.973 0 004 15v3H1v-3a3 3 0 013.75-2.906z"/>
                        </svg>
                        <div>
                          <p className="text-xs text-gray-400 font-semibold uppercase tracking-wide mb-1">Role</p>
                          <p className="text-sm text-gray-100 font-medium">Product Manager (Execution & Scale)</p>
                        </div>
                      </div>

                      {/* Timeline */}
                      <div className="flex items-start space-x-3">
                        <svg className="w-5 h-5 text-orange-400 flex-shrink-0 mt-0.5" fill="currentColor" viewBox="0 0 20 20" aria-hidden="true">
                          <path fillRule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-11a1 1 0 10-2 0v3.586L7.707 9.293a1 1 0 00-1.414 1.414l3 3a1 1 0 001.414 0l3-3a1 1 0 00-1.414-1.414L11 10.586V7z" clipRule="evenodd"/>
                        </svg>
                        <div>
                          <p className="text-xs text-gray-400 font-semibold uppercase tracking-wide mb-1">Timeline</p>
                          <p className="text-sm text-gray-100 font-medium">Discovery → MVP → Go-Live (~8–10 months)</p>
                        </div>
                      </div>

                      {/* Platform */}
                      <div className="flex items-start space-x-3 col-span-2">
                        <svg className="w-5 h-5 text-red-400 flex-shrink-0 mt-0.5" fill="currentColor" viewBox="0 0 20 20" aria-hidden="true">
                          <path d="M4 3a2 2 0 00-2 2v10a2 2 0 002 2h12a2 2 0 002-2V5a2 2 0 00-2-2H4zm12 12H4l4-8 3 6 2-4 3 6z"/>
                        </svg>
                        <div>
                          <p className="text-xs text-gray-400 font-semibold uppercase tracking-wide mb-1">Platform</p>
                          <p className="text-sm text-gray-100 font-medium">B2B | Web Application + Edge AI System</p>
                        </div>
                      </div>
                    </div>

                    {/* Summary */}
                    <div>
                      <p className="text-xs text-gray-400 font-semibold uppercase tracking-wide mb-2">Summary</p>
                      <p className="text-sm text-gray-300 leading-relaxed">
                        Led execution of an AI-powered computer vision platform enabling real-time detection of 16+ industrial safety violations. Delivered MVP and scaled to multi-site enterprise deployment, achieving ~80–85% production accuracy and enabling proactive incident prevention.
                      </p>
                    </div>
                  </>
                )}
              </div>
            </div>
          ))}
        </div>

        {/* Modal */}
        {modalOpen && selectedProject && (
          <div className="fixed inset-0 bg-black bg-opacity-50 backdrop-blur-sm flex items-center justify-center z-50 transition-opacity duration-300" onClick={() => setModalOpen(false)}>
            <div className="bg-white dark:bg-gray-800 rounded-xl p-8 max-w-4xl w-full mx-4 max-h-[90vh] overflow-y-auto relative shadow-2xl" onClick={(e) => e.stopPropagation()}>
              {/* Close Button */}
              <button
                onClick={() => setModalOpen(false)}
                className="absolute top-4 right-4 w-8 h-8 bg-gray-200 dark:bg-gray-700 rounded-full flex items-center justify-center text-gray-600 dark:text-gray-300 hover:bg-gray-300 dark:hover:bg-gray-600 transition-colors"
                aria-label="Close modal"
              >
                <svg
                  className="w-5 h-5"
                  fill="none"
                  stroke="currentColor"
                  viewBox="0 0 24 24"
                  aria-hidden="true"
                >
                  <path
                    strokeLinecap="round"
                    strokeLinejoin="round"
                    strokeWidth="2"
                    d="M6 18L18 6M6 6l12 12"
                  />
                </svg>
              </button>

              {/* Modal Content */}
              <div className="flex items-start space-x-4 mb-6">
                <div className="w-16 h-16 bg-blue-100 dark:bg-blue-900 rounded-lg flex items-center justify-center text-blue-600 dark:text-blue-400 flex-shrink-0">
                  {selectedProject.icon}
                </div>
                <div>
                  <h2 className="text-2xl font-bold text-gray-900 dark:text-white mb-2">
                    {selectedProject.title}
                  </h2>
                  <p className="text-lg text-blue-600 dark:text-blue-400 font-medium">
                    {selectedProject.category}
                  </p>
                </div>
              </div>

              {/* 30-Second Project Overview */}
              <div className="mb-8 bg-gradient-to-br from-blue-50 to-indigo-50 dark:from-gray-700 dark:to-gray-800 rounded-lg p-6 border border-blue-100 dark:border-gray-600 shadow-sm">
                <h3 className="text-sm font-semibold text-gray-900 dark:text-white mb-3">
                  30-Second Project Overview
                </h3>

                {/* Overview Metadata Grid */}
                <div className="grid grid-cols-1 md:grid-cols-2 gap-4 mb-6 pb-6 border-b border-blue-200 dark:border-gray-600">
                  {/* Product Name */}
                  <div className="flex items-start space-x-3">
                    <div className="w-6 h-6 flex items-center justify-center flex-shrink-0 mt-1">
                      <svg className="w-5 h-5 text-blue-600 dark:text-blue-400" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
                        <path strokeLinecap="round" strokeLinejoin="round" strokeWidth="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z" />
                      </svg>
                    </div>
                    <div>
                      <p className="text-[11px] font-semibold text-gray-600 dark:text-gray-400 uppercase tracking-wide">Product Name</p>
                      <p className="text-xs text-gray-900 dark:text-gray-100 font-medium">{selectedProject.title}</p>
                    </div>
                  </div>

                  {/* Domain */}
                  <div className="flex items-start space-x-3">
                    <div className="w-6 h-6 flex items-center justify-center flex-shrink-0 mt-1">
                      <svg className="w-5 h-5 text-green-600 dark:text-green-400" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
                        <path strokeLinecap="round" strokeLinejoin="round" strokeWidth="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4" />
                      </svg>
                    </div>
                    <div>
                      <p className="text-[11px] font-semibold text-gray-600 dark:text-gray-400 uppercase tracking-wide">Domain</p>
                      <p className="text-xs text-gray-900 dark:text-gray-100 font-medium">{selectedProject.title === "AI-Driven Safety Compliance Platform" ? "Industrial Safety & Enterprise Compliance" : selectedProject.title === "AI-Driven Music Metadata Enrichment Platform" ? "Media, Entertainment & Digital Content Intelligence" : "Domain"}</p>
                    </div>
                  </div>

                  {/* Role */}
                  <div className="flex items-start space-x-3">
                    <div className="w-6 h-6 flex items-center justify-center flex-shrink-0 mt-1">
                      <svg className="w-5 h-5 text-purple-600 dark:text-purple-400" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
                        <path strokeLinecap="round" strokeLinejoin="round" strokeWidth="2" d="M13 10V3L4 14h7v7l9-11h-7z" />
                      </svg>
                    </div>
                    <div>
                      <p className="text-[11px] font-semibold text-gray-600 dark:text-gray-400 uppercase tracking-wide">Role</p>
                      <p className="text-xs text-gray-900 dark:text-gray-100 font-medium">{selectedProject.title === "AI-Driven Safety Compliance Platform" ? "Product Manager (Execution & Scale)" : selectedProject.title === "AI-Driven Music Metadata Enrichment Platform" ? "Product Manager (Discovery, Execution & Productisation)" : "Product Manager"}</p>
                    </div>
                  </div>

                  {/* Timeline */}
                  <div className="flex items-start space-x-3">
                    <div className="w-6 h-6 flex items-center justify-center flex-shrink-0 mt-1">
                      <svg className="w-5 h-5 text-orange-600 dark:text-orange-400" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
                        <path strokeLinecap="round" strokeLinejoin="round" strokeWidth="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z" />
                      </svg>
                    </div>
                    <div>
                      <p className="text-[11px] font-semibold text-gray-600 dark:text-gray-400 uppercase tracking-wide">Timeline</p>
                      <p className="text-xs text-gray-900 dark:text-gray-100 font-medium">{selectedProject.title === "AI-Driven Safety Compliance Platform" ? "Discovery → MVP → Go-Live (~8–10 months)" : selectedProject.title === "AI-Driven Music Metadata Enrichment Platform" ? "Discovery → Algorithm Development → Productisation → Scale (~4 months)" : "Timeline"}</p>
                    </div>
                  </div>

                  {/* Platform */}
                  <div className="flex items-start space-x-3 md:col-span-2">
                    <div className="w-6 h-6 flex items-center justify-center flex-shrink-0 mt-1">
                      <svg className="w-5 h-5 text-red-600 dark:text-red-400" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true">
                        <path strokeLinecap="round" strokeLinejoin="round" strokeWidth="2" d="M9.75 17L9 20m0 0l-.75 3M9 20a6 6 0 1112 0m0 0l.75 3M15 20l.75 3" />
                      </svg>
                    </div>
                    <div>
                      <p className="text-xs font-semibold text-gray-600 dark:text-gray-400 uppercase tracking-wide">Platform</p>
                      <p className="text-sm text-gray-900 dark:text-gray-100 font-medium">{selectedProject.title === "AI-Driven Safety Compliance Platform" ? "B2B | Web Application + Edge AI System" : selectedProject.title === "AI-Driven Music Metadata Enrichment Platform" ? "B2B | Backend APIs + Search & Retrieval System" : "Platform"}</p>
                    </div>
                  </div>
                </div>

                {/* Summary Section */}
                <div>
                      <p className="text-[11px] font-semibold text-gray-600 dark:text-gray-400 uppercase tracking-wide mb-2">Summary</p>
                  <p className="text-sm text-gray-900 dark:text-gray-100 leading-relaxed">
                    {selectedProject.title === "AI-Driven Safety Compliance Platform" ? "Led execution of an AI-powered computer vision platform enabling real-time detection of 16+ industrial safety violations. Delivered MVP and scaled to multi-site enterprise deployment, achieving ~80–85% production accuracy and enabling proactive incident prevention." : selectedProject.title === "AI-Driven Music Metadata Enrichment Platform" ? "Led execution of an AI-powered metadata enrichment platform that automatically extracted rich semantic attributes from large-scale music video content. Enabled advanced search, filtering, and ranking across a catalog of ~50,000+ songs, significantly improving content discoverability and licensing readiness." : selectedProject.shortOverview}
                  </p>
                </div>
              </div>

              {/* Project Scope & Challenge */}
              <div className="mb-8">
                <h3 className="text-xl font-semibold text-gray-900 dark:text-white mb-4 border-b border-gray-200 dark:border-gray-700 pb-2">
                  Project Scope & Challenge
                </h3>
                <div className="text-gray-700 dark:text-gray-300 leading-relaxed" dangerouslySetInnerHTML={{ __html: selectedProject.context }} />
              </div>

              {/* My Role */}
              <div className="mb-8">
                <h3 className="text-xl font-semibold text-gray-900 dark:text-white mb-4 border-b border-gray-200 dark:border-gray-700 pb-2">
                  My Role, Product Strategy & Discovery
                </h3>
                <div className="text-gray-700 dark:text-gray-300 leading-relaxed" dangerouslySetInnerHTML={{ __html: selectedProject.role }} />
              </div>
            </div>
          </div>
        )}
      </div>
    </section>
  );
};

export default Projects;


